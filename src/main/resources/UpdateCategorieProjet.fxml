<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="tn.esprit.educareer.controllers.UpdateCategorieProjet"
            prefHeight="200.0" prefWidth="400.0">

    <VBox spacing="15.0" layoutX="30.0" layoutY="30.0">
        <Label text="Modifier la catégorie de projet"/>
        <TextField fx:id="tfCategorie" promptText="Nom de la catégorie"/>
        <Button text="Enregistrer" onAction="#modifierCategorie"/>
    </VBox>

</AnchorPane>
