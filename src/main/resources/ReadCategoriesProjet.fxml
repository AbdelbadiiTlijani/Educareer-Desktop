<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="tn.esprit.educareer.controllers.ReadCategoriesProjet"
            prefHeight="400.0" prefWidth="600.0">

    <VBox spacing="10.0" AnchorPane.topAnchor="10.0" AnchorPane.leftAnchor="10.0"
          AnchorPane.rightAnchor="10.0" AnchorPane.bottomAnchor="10.0">
        <Button fx:id="btnAjouter" text="Ajouter Catégorie" onAction="#goToAjoutCategorie"/>
        <TableView fx:id="tableCategorie" prefHeight="350.0" prefWidth="580.0">
            <columns>

                <TableColumn fx:id="colCompteur" text="num" prefWidth="50.0"/>

                <TableColumn fx:id="colNom" text="Nom de la catégorie" prefWidth="200.0"/>

                <TableColumn fx:id="colActions" text="Actions" prefWidth="200.0"/>


            </columns>
        </TableView>
    </VBox>

</AnchorPane>
